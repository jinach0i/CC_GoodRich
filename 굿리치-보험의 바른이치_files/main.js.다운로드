// contents of main:
require.config({
	urlArgs: function(id, url) {
		if (url.indexOf('/js/') !== -1 || url.indexOf('/css/') !== -1 || url.indexOf('/TouchEn/') !== -1) {
			return "";
		} else {
			var args = "bust=" + (new Date()).getTime();
			return (url.indexOf('?') === -1 ? '?' : '&') + args;
		}
	},
